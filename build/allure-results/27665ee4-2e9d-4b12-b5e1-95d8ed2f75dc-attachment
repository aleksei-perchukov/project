    <script type="text/javascript">
        require(
            [
                'jquery'
            ],
            function (
                $
            ) {
                $(window).on('ajax:addToCart', function(e, response){
                    if (typeof(response.response.raptor) != 'undefined') {
                        let trackingData = prepareTrackingData(response.response);
                        raptor.push("trackEvent", trackingData)

                        console.log('[RaptorSmartAdvisor::trackAddToCart]', trackingData);
                    }
                });

                $(window).on('ajax:cartQtyUpdate', function(e, response){
                    if (typeof(response.response.raptor) != 'undefined') {
                        let trackingData = prepareTrackingData(response.response);
                        raptor.push("trackEvent", trackingData)

                        console.log('[RaptorSmartAdvisor::trackCartQtyUpdate]', trackingData);
                    }
                });

                $(window).on('ajax:cartDeleteItem', function(e, response){
                    if (typeof(response.response.responseText) != 'undefined') {
                        let res = JSON.parse(response.response.responseText);
                        if (typeof(res.raptor) != 'undefined') {
                            let trackingData = prepareTrackingData(res);
                            raptor.push("trackEvent", trackingData);

                            console.log('[RaptorSmartAdvisor::trackCartDeleteItem]', trackingData);
                        }
                    }
                });

                function prepareTrackingData(response) {
                    return {
                        p1:"basket",
                        p10:response.raptor.cart_content,
                        p11:response.raptor.cart_id
                    };
                }
            }
        );
    </script>



