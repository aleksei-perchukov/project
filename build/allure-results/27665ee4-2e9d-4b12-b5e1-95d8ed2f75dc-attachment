<script>
    require([
        'jquery',
        'uiRegistry',
        'BelVG_MageWorxUrls/js/catalog/product/url-updater'
    ], function($, registry, optionUrlUpdater) {

        registry.get('mageworxOptionBase', function(optionBase) {
            var order = 1300,
                config = {};
            if (optionBase) {
                optionBase.addUpdater(order, optionUrlUpdater(config));
            } else {
                var updaters = registry.get('mageworxOptionUpdaters');
                if (!updaters) {
                    updaters = {};
                }
                updaters[order] = optionUrlUpdater(config);
                registry.set('mageworxOptionUpdaters', updaters);
            }
        });

    });
</script>
